{
  "name": "docker-binding",
  "version": "0.1.0",
  "description": "a typescript docker remote api wrapper base on got",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "dependencies": {
    "got": "^8.3.0"
  },
  "devDependencies": {
    "@types/jest": "^22.2.2",
    "@types/node": "^9.6.2",
    "jest": "^22.4.3",
    "ts-jest": "^22.4.2",
    "typescript": "^2.4.1"
  },
  "scripts": {
    "test": "jest",
    "build": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Dreamacro/docker-binding.git"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testPathIgnorePatterns": [
      "<rootDir>/test/spec_helper.ts"
    ],
    "testMatch": [
      "**/test/*.(ts|tsx|js)"
    ]
  },
  "author": "Dreamacro",
  "license": "MIT"
}
